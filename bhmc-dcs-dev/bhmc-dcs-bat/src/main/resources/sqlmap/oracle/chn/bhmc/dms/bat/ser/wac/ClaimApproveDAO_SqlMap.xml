<mapper namespace="chn.bhmc.dms.bat.ser.wac.service.dao.ClaimApproveDAO">

    <parameterMap id="paramMap" type="hashMap">
        <parameter property="batDay" mode="IN" jdbcType="NUMERIC" javaType="java.lang.Integer" />
        <parameter property="batDlrCd" mode="IN" jdbcType="VARCHAR" javaType="java.lang.String" />
    </parameterMap>

    <!-- 판정 결과를 실행한다. -->
    <select id="insertClaimApprove" statementType="CALLABLE" parameterType="int">
         { call SP_SER_BWMS_CLAIM_APPROVE(
           #{batDay, mode=IN, jdbcType=NUMERIC}
         ) }
    </select>

    <!-- 판정 결과를 재실행한다. -->
    <select id="insertReClaimApprove" statementType="CALLABLE" parameterType="hashMap" >
         { call SP_SER_BWMS_CLAIM_RE_APPROVE(
           #{batDay, mode=IN, jdbcType=NUMERIC}
          ,#{batDlrCd, mode=IN, jdbcType=VARCHAR}
         ) }
    </select>

</mapper>