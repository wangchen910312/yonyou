<mapper namespace="chn.bhmc.dms.bat.cmm.sci.service.dao.CommonCodeIfBatchDAO">

    <!-- 공통코드그룹 테이블[CM_0301T]에서 공통코드그룹  송신 테이블[CM_0301IS]에 반영한다. -->
    <update id="insertCommonCodeGroupCodeIf" parameterType="int" statementType="CALLABLE" >
         {call SP_COM_DCS_DMS_CM_0301T(
            #{value, mode=IN, jdbcType=NUMERIC}
         )}
    </update>

    <!-- 공통코드그룹 텍스트 테이블[CM_0302T]에서 공통코드그룹 텍스트 송신 테이블[CM_0302IS]에 반영한다. -->
    <update id="insertCommonCodeGroupTextIf" statementType="CALLABLE" >
         {call SP_COM_DCS_DMS_CM_0302T}
    </update>

    <!-- 공통코드 테이블[CM_0303T]에서 공통코드  송신 테이블[CM_0303IS]에 반영한다. -->
    <update id="insertCommonCodeIf" parameterType="int" statementType="CALLABLE" >
         {call SP_COM_DCS_DMS_CM_0303T(
            #{value, mode=IN, jdbcType=NUMERIC}
         )}
    </update>

    <!-- 공통코드 텍스트 테이블[CM_0304T]에서 공통코드 텍스트  송신 테이블[CM_0304IS]에 반영한다. -->
    <update id="insertCommonCodeTextIf" statementType="CALLABLE" >
         {call SP_COM_DCS_DMS_CM_0304T}
    </update>
</mapper>