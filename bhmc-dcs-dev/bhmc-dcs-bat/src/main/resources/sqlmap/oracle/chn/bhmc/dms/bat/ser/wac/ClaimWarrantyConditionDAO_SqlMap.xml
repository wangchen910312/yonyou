<mapper namespace="chn.bhmc.dms.bat.ser.wac.service.dao.ClaimWarrantyConditionDAO">


    <!-- BWMS 차종별 보증 정보  데이터를 가져온다.-->
    <select id="insertBwmsLtsWtyConditionInfo" statementType="CALLABLE" >
         { call SP_SER_BWMS_WTY_LTS_CONDITION  }
    </select>

    <!-- BWMS 빈별 보증 정보  데이터를 가져온다.-->
    <select id="insertBwmsVinConditionInfo" statementType="CALLABLE" >
         { call SP_SER_BWMS_WTY_VIN_CONDITION  }
    </select>

    <!-- BWMS 스페셜빈별 정보  데이터를 가져온다.-->
    <select id="insertBwmsSpecialVinInfo" statementType="CALLABLE" >
         { call SP_SER_BWMS_SPECIAL_VIN  }
    </select>

</mapper>