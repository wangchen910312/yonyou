<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT A.DLR_CD
           ,A.MEMBERSHIP_NO
           ,A.CUST_NO
           ,A.VIN_NO -- VIN번호
           ,A.GRADE_CD -- 고객등급
           ,B.CUST_NM --고객명
           ,C.SEQ
           ,C.PKG_SEQ
           ,C.PKG_NM -- 패키지명
           ,C.PKG_CONT --패키지내용
           ,C.CUPN_SEQ 
           ,C.CUPN_NM -- 쿠폰명
           ,C.CUPN_CONT -- 쿠폰내용
           ,C.CUPN_DEST_DT -- 쿠폰소멸일
           ,C.CUPN_USE_YN -- 쿠폰사용여부
           ,C.CUPN_USE_DT -- 쿠폰사용일
           ,C.USE_DEPT_NM -- 사용부서명
           ,C.USE_PRSN_ID -- 사용담당자ID
           ,C.CUPN_NO --쿠폰번호
           ,C.REG_DT --구매일
FROM    CR_0801T A LEFT OUTER JOIN CR_0101T B
          ON A.DLR_CD = B.DLR_CD
          AND A.CUST_NO = B.CUST_NO
          INNER JOIN CR_0816T C
          ON A.DLR_CD = C.DLR_cD
          AND A.MEMBERSHIP_NO = C.MEMBERSHIP_NO
WHERE A.DLR_CD = 'A07AA' -- 딜러코드
${IF(LEN(txtCardNum) == 0, ""," and A.VIN_NO = '"+txtCardNum+"'")} 
${IF(LEN(txtCustNm) == 0, ""," and B.CUST_NM LIKE '"+txtCustNm+"'")} 

${IF(LEN(dateBuyStart) = 0, ""," and TO_CHAR(C.REG_DT,'YYYYMMDD') between REPLACE('"+dateBuyStart+"', '-', '') and REPLACE('"+dateBuyEnd+"', '-', '')") } 
${IF(LEN(dateUseStart) = 0, ""," and TO_CHAR(C.CUPN_USE_DT,'YYYYMMDD') between REPLACE('"+dateUseStart+"', '-', '') and REPLACE('"+dateUseEnd+"', '-', '')") } ]]></Query>
</TableData>
