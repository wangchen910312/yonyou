<Query>
<![CDATA[SELECT 
  A.BIZ_CD,
  B.CUST_NM,
  case
    when A.ref_table_nm = 'CR_0502T' then ( select B.CUST_CARE_NM from cr_0501t B where A.ref_key_nm = B.cust_care_seq and A.ref_table_nm = 'CR_0502T' )
    when A.ref_table_nm = 'CR_0710T' then ( select B.MAK_NM from cr_0701t B where A.ref_key_nm = B.MAK_CD and A.ref_table_nm = 'CR_0710T' )
    when A.ref_table_nm = 'CR_0905T' then ( select B.STSF_IVST_NM from cr_0901t B where A.ref_key_nm = B.SEQ and A.ref_table_nm = 'CR_0905T' )
    end 
  as TITLE, 
  A.TEL_NO,
  A.HP_NO,
  A.REAL_CALL_DT,
  A.CALL_PRSN_ID,
  A.REG_DT
    
FROM CR_0606T A
inner join CR_0101T B on A.cust_no = B.cust_no
where 1=1
and A.dlr_cd = 'A07AA'                       -- 세션값 이용, 'A07AA'
and TO_CHAR(A.REG_DT,'YYYYMMDD') = REPLACE('${dateStart}', '-', '')  -- 등록일
${IF(LEN(ComboCallEmployee) == 0, ""," and A.CALL_PRSN_ID = '"+ComboCallEmployee+"'")} 
${IF(LEN(ComboCallEmployee) == 0, ""," and A.CALL_PRSN_ID = '"+ComboCallEmployee+"'")} 
]]></Query>
</TableData>
