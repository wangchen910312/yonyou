<Query>
<![CDATA[    SELECT DM.DLR_CD
          ,DM.DLR_SHT_NM AS DLR_NM
          ,SDPT.DIVI_NM AS SDPT_NM
          ,OFFICE.OFF_NM
          ,SUNG.CSTL_NM AS SUNG_NM
          ,CITY.CITY_NM
          ,T1.MAK_NM AS COL01 -- 캠페인명
          ,T1.BUDGET_AMT AS COL02 -- 예상비용
          , (SELECT COUNT(*) FROM CR_0223T WHERE DLR_CD = T1.DLR_CD AND HOLD_TP = '03' AND HOLD_DETL_TP_SEQ = HOLD.HOLD_SEQ) AS COL03      -- 리드량
          , (SELECT COUNT(*) FROM CR_0223T WHERE DLR_CD = T1.DLR_CD AND HOLD_TP = '03' AND HOLD_DETL_TP_SEQ = HOLD.HOLD_SEQ AND LEAD_STAT_CD = '06') AS COL04      --  거래량
          ,50 AS COL05      -- 내방예약원가 (예상비용/리드량)
          ,60 AS COL06      --  거래원가 (예상비용/거래량
          , HOLD.HOLD_SEQ
    FROM CR_0701T T1
    INNER JOIN CM_0101T DM ON T1.DLR_CD = DM.DLR_CD
    INNER JOIN CM_2701T SUNG ON DM.SALE_DLR_SUNG_CD = SUNG.CSTL_CD
    INNER JOIN CM_2501T SDPT ON DM.SDPT_CD = SDPT.DIVI_CD
    INNER JOIN CM_2601T OFFICE ON DM.DIST_OFFICE_CD = OFFICE.OFF_CD
    INNER JOIN CM_3201T CITY ON DM.SALE_DLR_CITY_CD = CITY.CITY_CD
    LEFT OUTER JOIN CR_0215T HOLD ON T1.DLR_CD = HOLD.DLR_CD AND T1.MAK_CD = HOLD.REMARK
    WHERE 1=1
    AND T1.HOST_CD = '02'  -- CRM708 (01:제조사, 02:딜러)
    AND DM.DMS_USE_YN = 'Y'
    --AND TO_CHAR(T1.START_DT,'YYYY-MM-DD') BETWEEN '2000-01-01' AND '2018-01-30'
    ${IF(LEN(sSearchFromDt) = 0 , ""," AND TO_CHAR(T1.START_DT,'YYYY-MM-DD') BETWEEN '"+sSearchFromDt+"' AND '"+sSearchToDt+"'") }
    ${IF(LEN(sDlrCd) <= 0,""," AND DM.DLR_CD = '" + sDlrCd + "'")}
    ${IF(LEN(sDivision) <= 0,""," AND DM.SDPT_CD = '" + sDivision + "'")}
    ${IF(LEN(sOffice) <= 0,""," AND DM.DIST_OFFICE_CD = '" + sOffice + "'")}
    ${IF(LEN(sSung) <= 0,""," AND DM.SALE_DLR_SUNG_CD = '" + sSung + "'")}
    ${IF(LEN(sCity) <= 0,""," AND DM.SALE_DLR_CITY_CD = '" + sCity + "'")}
    ORDER BY T1.DLR_CD ASC, T1.MAK_NM ASC]]></Query>
</TableData>
</TableDataMap>
