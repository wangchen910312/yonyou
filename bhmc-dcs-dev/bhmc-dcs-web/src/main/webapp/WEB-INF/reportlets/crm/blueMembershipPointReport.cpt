<TableData name="blueMembershipPointReport" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="txtCardNum"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="txtTelephoneNum"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="txtCustNm"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select cust_nm, hp_no, BLUE_MEMBERSHIP_NO, BLUE_MEMBERSHIP_JOIN_DT
     , (select sum(decode(point_tp,'+', occr_point_val, 0)) 
          from cr_0808t pt 
          where pt.BLUE_MEMBERSHIP_NO = BLUE_MEMBERSHIP_NO ) sumPlusPoint
     , (select sum(decode(point_tp,'-', occr_point_val, 0)) 
          from cr_0808t pt 
          where pt.BLUE_MEMBERSHIP_NO = BLUE_MEMBERSHIP_NO ) sumMinusPoint
  from cr_0101t
 where dlr_cd = 'A07AA' 
${IF(LEN(txtCustNm) == 0, ""," and and cust_nm like '%'||'"+txtCustNm+"'||'%'")} 
${IF(LEN(txtCardNum) == 0, ""," and BLUE_MEMBERSHIP_NO like '%'||'"+txtCardNum+"'||'%'")} 
${IF(LEN(txtTelephoneNum) == 0, ""," and hp_no like '%'||'"+txtTelephoneNum+"'||'%'")}    ]]></Query>
</TableData>
