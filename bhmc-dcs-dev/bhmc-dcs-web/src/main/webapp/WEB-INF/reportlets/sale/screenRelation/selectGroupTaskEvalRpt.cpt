<TableData name="mainDs" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="sDlrCd"/>
<O>
<![CDATA[D0103]]></O>
</Parameter>
<Parameter>
<Attributes name="sReqSeq"/>
<O>
<![CDATA[3]]></O>
</Parameter>
<Parameter>
<Attributes name="sUsrId"/>
<O>
<![CDATA[D0030847]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[/*집단판매업무 세부내용*/
SELECT A.DLR_CD                  --딜러코드
     , B.DLR_NM
     , A.USR_ID                  --사용자ID
     , A.REQ_SEQ                 --신청번호
     , A.REQ_START_DT            -- 요청시작일
     , A.REQ_END_DT              -- 요청종료일
     , A.REQ_DT                  -- 요청일
     , A.CUST_NM                 -- 고객명
     , (
     /*공통코드명칭 가져오기*/
	SELECT Z1.CMM_CD_NM
	  FROM (SELECT A.CMM_CD
         		      , NVL(B.CMM_CD_TXT, A.CMM_CD_NM) AS CMM_CD_NM
	          FROM CM_0303T A
               LEFT OUTER JOIN CM_0304T B
     	    	       ON A.CMM_GRP_CD = B.CMM_GRP_CD
		           AND A.CMM_CD = B.CMM_CD
     	      	 AND B.LANG_CD = 'zh' --중국어로 고정
	          WHERE A.CMM_GRP_CD = 'SAL151' --변수
       		  AND A.USE_YN = 'Y'
		    ) Z1
	 WHERE CMM_CD =  A.ATC_DSTIN_CD --변수
     ) AS ATC_DSTIN_CD            --항목
     , A.ATC_INTRO_CONT          --항목소개
     , A.FEE_POLICY_CONT         --수수료상관정책
     , A.DMS_OPEN_YN             --DMS 공시여부
     , FN_CMM_CD_NM('COM020',A.POA_OFFER_YN,'zh','N') AS POA_OFFER_YN   --위임장 제공여부
     , A.APPROVE_CD              --상태
     , A.DEL_YN                  --삭제여부
     , A.APPLY_START_DT          --임명시작일자
     , A.APPLY_END_DT            --임명종료일자
     , A.REQ_SAL_NO
  FROM SA_0411T A               /* 집단판매업무권한신청정보 */
  LEFT OUTER JOIN CM_0101T B
    ON A.DLR_CD = B.DLR_CD
 WHERE A.DLR_CD = '${sDlrCd}'
   AND A.USR_ID = '${sUsrId}'
   AND A.REQ_SEQ = '${sReqSeq}']]></Query>
</TableData>
<TableData name="CarInfoDs" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="sDlrCd"/>
<O>
<![CDATA[D0103]]></O>
</Parameter>
<Parameter>
<Attributes name="sReqSeq"/>
<O>
<![CDATA[3]]></O>
</Parameter>
<Parameter>
<Attributes name="sUsrId"/>
<O>
<![CDATA[D0030847]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[/*집단판매업무 신청차량정보*/
SELECT A.DLR_CD || A.USR_ID || A.REQ_SEQ || A.CARLINE_CD AS PK
     , A.DLR_CD              --딜러코드
     , A.USR_ID              --사용자ID
     , A.REQ_SEQ             --요청번호
     , A.CARLINE_CD          --차종코드
     , FN_GET_CARLINE_NM(A.CARLINE_CD) AS CARLINE_NM
     , A.MODEL_CD            --모델코드
     , CASE WHEN LENGTH(A.MODEL_CD) = 5
            THEN FN_GET_FSC_NM(A.CARLINE_CD,A.MODEL_CD)
            ELSE FN_GET_MODEL_NM(A.MODEL_CD)
            END  AS MODEL_NM
     , A.APP_QTY             --수량
     , A.REQ_RC              --고객요구D/C
     , A.REQ_QUAR            --스펙요구
     , A.REG_USR_ID          --등록자ID
     , A.REG_DT              --등록일자
     , A.UPDT_USR_ID         --수정자ID
     , A.UPDT_DT             --수정일자
  FROM SA_0443T A    --집단판매업무권한신청-차량내역
 WHERE A.DLR_CD       = '${sDlrCd}'   --딜러코드
   AND A.USR_ID       = '${sUsrId}'   --사용자ID
   AND A.REQ_SEQ      = '${sReqSeq}'  --요청번호]]></Query>
</TableData>
<TableData name="evalInfoDs" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="sDlrCd"/>
<O>
<![CDATA[D0103]]></O>
</Parameter>
<Parameter>
<Attributes name="sReqSeq"/>
<O>
<![CDATA[3]]></O>
</Parameter>
<Parameter>
<Attributes name="sUsrId"/>
<O>
<![CDATA[D0030847]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[/*집단판매업무 심사정보*/
SELECT A.DLR_CD              --딜러코드
     , A.USR_ID              --사용자ID
     , A.REQ_SEQ             --요청번호
     , A.REQ_NO              --요청일련번호
     /*심사자명 가져오기*/
	,( SELECT B.USR_NM 
	     FROM CM_0801T B
	    WHERE --B.DLR_CD = '${sDlrCd}'
	       B.USR_ID = A.PEVAL_ID) 
       AS PEVAL_ID         --심사자ID
     , A.EVAL_DT             --심사일자
     ,FN_CMM_CD_NM('SAL131',EVAL_RSLT_YN, 'zh' ,'N') AS EVAL_RSLT_YN        --심사결과여부
     , A.REMARK              --비고
     , A.REG_USR_ID          --등록자ID
     , A.REG_DT              --등록일자
     , A.UPDT_USR_ID         --수정자ID
     , A.UPDT_DT             --수정일자
  FROM SA_0410T A    --집단판매업무권한심사
 WHERE A.DLR_CD       = '${sDlrCd}'         --딜러코드
   AND A.USR_ID       = '${sUsrId}'         --사용자ID
   AND A.REQ_SEQ      = '${sReqSeq}'        --요청번호
 ORDER BY REG_DT DESC]]></Query>
</TableData>
