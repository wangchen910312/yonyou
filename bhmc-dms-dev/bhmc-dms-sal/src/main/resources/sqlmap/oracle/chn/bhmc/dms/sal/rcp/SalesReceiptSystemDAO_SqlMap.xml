<mapper namespace="chn.bhmc.dms.sal.rcp.service.dao.SalesReceiptSystemDAO">

    <!-- 2021-04-29 宫荣枢 查询经销商上线发票系统标识 -->
    <select id="selectSalesReceiptSystemData" parameterType="java.lang.String" resultType="SalesReceiptSystemVO">
    /* [chn.bhmc.dms.sal.rcp.service.dao.SalesReceiptSystemDAO.selectSalesReceiptSystemData] */
    	SELECT receiptSystem.DLR_CD, receiptSystem.RECEIPT_SYSTEM_FLAG
        FROM SA_0512T receiptSystem
        WHERE receiptSystem.DLR_CD = #{DLR_CD}
    </select>
