<Query>
<![CDATA[SELECT FN_GET_USER_NM(MNG_SC_ID) -- 판매고문						
     , COUNT(SUBMIT_DT) -- 신규수량						
     , COUNT(FAIL_DT) -- 전패수량						
  FROM (						
 SELECT OPPT.C_SEQ,						
        OPPT.LEAD_STAT_CD,						
        OPPT.SUBMIT_DT,						
        NULL AS FAIL_DT,						
        OPPT.MNG_SC_ID						
   FROM CR_0223T OPPT						
  WHERE --OPPT.DLR_CD = 'D0103'		
    1 = 1	
    ${IF(LEN(sDlrCdParam) == 0, ""," and OPPT.DLR_CD = '"+sDlrCdParam+"'")}				
    AND OPPT.LEAD_STAT_CD = '04'						
  UNION ALL						
 SELECT C_SEQ,						
        LEAD_STAT_CD,						
        REG_DT,						
        SUBMIT_DT,						
        MNG_SC_ID						
   FROM ( SELECT OPPT.C_SEQ,						
                 OPPT.LEAD_STAT_CD,						
                 HT.REG_DT, -- 신규일						
                 OPPT.SUBMIT_DT, -- 전패일						
                 OPPT.MNG_SC_ID,						
                 ROW_NUMBER( ) OVER (PARTITION BY OPPT.SEQ ORDER BY HT.REG_DT DESC ) AS RN					
            FROM CR_0223T OPPT,						
                 CR_0211HT HT						
           WHERE --OPPT.DLR_CD = 'D0103'				
           1 = 1	
           ${IF(LEN(sDlrCdParam) == 0, ""," and OPPT.DLR_CD = '"+sDlrCdParam+"'")}		
             AND OPPT.LEAD_STAT_CD IN('09', '08') -- CRM375 08:전출, 09:전패
             AND OPPT.BEF_LEAD_STAT_CD = '04'						
             AND OPPT.SEQ = HT.SALE_OPPT_SEQ	
             AND OPPT.C_SEQ = HT.C_SEQ			
             AND HT.LEAD_STAT_CD = '04' )
  WHERE RN = 1						
)					
WHERE --SUBMIT_DT BETWEEN SYSDATE - 100 AND SYSDATE -- 검색 : 제출시간	
	1 = 1
   ${IF(LEN(sRegStartDttParam) = 0 , ""," and TO_CHAR(SUBMIT_DT,'"+sDateFormat+"') between '"+sRegStartDttParam+"' and '"+sRegEndDtParam+"'") }					
GROUP BY MNG_SC_ID						
ORDER BY MNG_SC_ID						
]]></Query>
</TableData>
