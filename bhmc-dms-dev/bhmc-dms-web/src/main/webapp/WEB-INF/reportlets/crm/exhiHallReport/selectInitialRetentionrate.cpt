<Query>
<![CDATA[SELECT C.VSIT_MONTH AS NAME,
       NVL(AB.CNT, 0) AS CNT,
       NVL(AB.DEN, 0) AS DEN,
       NVL(AB.RATIO, 0) RATIO
     FROM (
          SELECT DISTINCT TO_CHAR(T.ROWDATE, 'yyyy-MM') VSIT_MONTH
          FROM (
               SELECT ROWNUM,
                       TO_DATE('${startDt}', 'yyyy-MM') + ROWNUM - 1 ROWDATE
                  FROM DUAL
                CONNECT BY ROWNUM < TO_DATE('${endDt}', 'yyyy-MM') - TO_DATE('${startDt}', 'yyyy-MM') + 2) T
         ORDER BY VSIT_MONTH DESC) C
         LEFT JOIN (
                    SELECT A.VSIT_MONTH AS NAME,
                    A.MOLECULE AS CNT,
                    B.DENOMINATOR AS DEN,
                    NVL(CASE
                          WHEN A.MOLECULE > 0 AND B.DENOMINATOR > 0 THEN
                           A.MOLECULE / B.DENOMINATOR
                          ELSE
                           0
                        END,
                        0) AS RATIO
               FROM (SELECT TO_CHAR(T1.VSIT_DT, 'YYYY-MM') AS VSIT_MONTH,
                            COUNT(*) AS MOLECULE  --分子
                       FROM CR_0214T T1
                      WHERE T1.DEL_YN = 'N'
                        AND T1.DLR_CD = '${sDlrCd}'
                        --AND T1.VSIT_DT >= TO_DATE('${sStartDt}', 'yyyy-MM-dd') --到店日期 
                        --AND T1.VSIT_DT <= TO_DATE('${sEndDt}', 'yyyy-MM-dd') --到店日期   
                        AND TO_CHAR(VSIT_DT,'YYYYMMDD')  BETWEEN '${sStartDt}' AND '${sEndDt}'
                        AND T1.INFO_TP = '01' --自然进店
                        AND T1.HP_NO IS NOT NULL
                        AND FN_GET_CMMCD_NM('CRM380', T1.RVSIT_YN, 'zh') = '首次到店'
                      GROUP BY TO_CHAR(T1.VSIT_DT, 'YYYY-MM')) A,
                    (SELECT TO_CHAR(T1.VSIT_DT, 'YYYY-MM') VSIT_MONTH,
                            COUNT(*) AS DENOMINATOR  --分母
                       FROM CR_0214T T1
                      WHERE T1.DEL_YN = 'N'
                        AND T1.DLR_CD = '${sDlrCd}'
                        --AND T1.VSIT_DT >= TO_DATE('${sStartDt}', 'yyyy-MM-dd') --到店日期 
                        --AND T1.VSIT_DT <= TO_DATE('${sEndDt}', 'yyyy-MM-dd') --到店日期 
                        AND TO_CHAR(VSIT_DT,'YYYYMMDD')  BETWEEN '${sStartDt}' AND '${sEndDt}'
                        AND T1.INFO_TP = '01' --自然进店
                        --AND FN_GET_CMMCD_NM('CRM380', T1.RVSIT_YN, 'zh') = '首次到店'
                      GROUP BY TO_CHAR(T1.VSIT_DT, 'YYYY-MM')) B
              WHERE A.VSIT_MONTH = B.VSIT_MONTH ) AB
    ON C.VSIT_MONTH = AB.NAME 
      ORDER BY C.VSIT_MONTH ]]></Query>
</TableData>
</TableDataMap>
