<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT REG_USR_ID
     , FN_GET_USER_NM(REG_USR_ID) AS REG_USR_NM
     , ROUND(SUM(CALL_MI) / COUNT(SEQ)) AS CALL_AVG
	FROM (
		SELECT T4.REG_USR_ID
		      , (T4.CALL_END_DT - T4.CALL_START_DT) * 24 * 60 * 60 AS CALL_MI
		      , T4.SEQ
		FROM CR_0213T T4, (
				SELECT
				MIN(T3.SEQ) KEEP(DENSE_RANK FIRST ORDER BY T3.SALE_OPPT_SEQ) AS SEQ
				, T3.SALE_OPPT_SEQ
				FROM (	 SELECT 
					      CALL.SEQ
					      , CALL.SALE_OPPT_SEQ
					   FROM CR_0223T OPPT 
					  INNER JOIN CR_0213T CALL ON OPPT.DLR_CD = CALL.DLR_CD AND CALL.C_SEQ = OPPT.C_SEQ
					  INNER JOIN CR_0101T C ON OPPT.DLR_CD = C.DLR_CD AND OPPT.CUST_NO = C.CUST_NO
					  WHERE OPPT.DLR_CD = '${sDlrCd}'
					    AND OPPT.INFO_TP = '02' --IDCC    
					    AND CALL.CALL_START_DT IS NOT NULL 
				        ${IF(LEN(sRegStartDt) = 0 , "", " AND TO_CHAR(OPPT.LEAD_START_DT,'"+sDateFormat+"') BETWEEN '"+sRegStartDt+"' AND '"+sRegEndDt+"'") } --리드시작일
				        ${IF(LEN(sLeadStatCd) = 0 , "", " AND CALL.LEAD_STAT_CD = '"+sLeadStatCd+"'") } -- 리드상태
					   ${IF(LEN(sValidGradeCd) = 0 , "", " AND OPPT.VALID_GRADE_CD = '"+sValidGradeCd+"'") } -- 유효여부
				) T3
				GROUP BY T3.SALE_OPPT_SEQ
			) T5
		WHERE T4.DLR_CD = '${sDlrCd}'
		  AND T4.SEQ = T5.SEQ
	)
GROUP BY REG_USR_ID;
]]></Query>
</TableData>
</TableDataMap>
