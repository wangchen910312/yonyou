<Query>
<![CDATA[ SELECT (SELECT A.OPPT_ING_CNT
            FROM CR_0222T A
           WHERE A.DLR_CD = DLR_CD
             AND A.STATS_DT = TO_CHAR(ADD_MONTHS(TO_DATE('${sDt}', '${sMonthFormat}'), -1), 'YYYYMM') ) AS OPPT_INT_CNT,
        DECODE('${sDt}', TO_CHAR(TRUNC(SYSDATE, 'MM'), '${sMonthFormat}'),
            ( SELECT COUNT(*)
                FROM CR_0223T 
               WHERE DLR_CD = '${sDlrCd}'
                 AND LEAD_STAT_CD = '04'
                 AND REG_DT BETWEEN TRUNC(SYSDATE,'MM') AND SYSDATE),
        (  SELECT A.OPPT_NEW_CNT
             FROM CR_0222T A
            WHERE A.DLR_CD = DLR_CD
              AND A.STATS_DT = TO_CHAR(TO_DATE('${sDt}','${sMonthFormat}'),'yyyyMM') ) ) AS OPPT_NEW_CNT,
        DECODE('${sDt}', TO_CHAR(TRUNC(SYSDATE, 'MM'), '${sMonthFormat}'),
        ( SELECT COUNT(*)
             FROM CR_0223T
            WHERE DLR_CD = '${sDlrCd}'
              AND LEAD_STAT_CD IN('08', '09')
              AND SUBMIT_DT BETWEEN TRUNC(SYSDATE, 'MM') AND SYSDATE ),
        (  SELECT A.OPPT_FAIL_CNT
             FROM CR_0222T A
            WHERE A.DLR_CD = DLR_CD
              AND A.STATS_DT = TO_CHAR(TO_DATE('${sDt}','${sMonthFormat}'),'yyyyMM') ) ) AS OPPT_FAIL_CNT
   FROM DUAL]]></Query>
</TableData>
