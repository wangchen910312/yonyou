<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[BHDMS]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
SELECT SALE_EMP_NO
     , (SELECT USR_NM FROM CM_0801T WHERE USR_ID = SALE_EMP_NO) AS SALE_EMP_NM  -- 판매고문명
     , MONTH_CNT_B1         -- 전월대비
     , MONTH_CNT            -- 본월
     , ROUND(CASE WHEN MONTH_CNT_B1 = 0
                  THEN MONTH_CNT * 100
                  ELSE (MONTH_CNT-MONTH_CNT_B1)/MONTH_CNT_B1 * 100
              END
       ,1) AS  SALE_RATE   -- 전월대비(%)
 FROM (
    SELECT A.SALE_EMP_NO
             , SUM(DECODE(DL_MM, DL_DT_B1, CAR_SALE_CNT, 0)) AS MONTH_CNT_B1
             , SUM(DECODE(DL_MM, DL_DT, CAR_SALE_CNT, 0)) AS MONTH_CNT
      FROM (
           SELECT SALE_EMP_NO
                 , DL_MM
                 , SUM(CAR_SALE_CNT) AS CAR_SALE_CNT
              FROM (
                SELECT SALE_EMP_NO
                     , TO_CHAR(REAL_DL_DT, 'YYYYMM') AS DL_MM
                     , SUM(1) AS CAR_SALE_CNT
                  FROM SA_0201T
                 WHERE DLR_CD = '${sDlrCd}'              -- 딜러코드 : 화면에서 받는 인자값
                   AND CONTRACT_STAT_CD = '50'
                   AND TO_CHAR(REAL_DL_DT, 'YYYYMMDD') BETWEEN TO_CHAR(ADD_MONTHS(TO_DATE('${sEndDt}', 'YYYYMMDD'), -1), 'YYYYMM')||'01'
                                                           AND '${sEndDt}'
                   GROUP BY SALE_EMP_NO, TO_CHAR(REAL_DL_DT, 'YYYYMM')
               UNION
                  SELECT T2.USR_ID AS SALE_EMP_NO
                       , '${LEFT(sEndDt,6)}' AS DL_MM  --검색조건  : 소매일자 YYYYMM
                       , 0  AS CAR_SALE_CNT
                    FROM CM_0803T T1
                    INNER JOIN CM_0801T T2 ON T1.REF_ID = T2.USR_ID AND T1.REF_TP = 'U'
                    WHERE T1.SYS_CD = 'D'
                      AND T1.ROLE_ID = 'ROLE_SALES_ADVISOR'
                      AND T2.DLR_CD =  '${sDlrCd}'        -- 딜러코드 : 화면에서 받는 인자값
                      AND T1.USE_YN = 'Y'
                      AND T2.DEL_YN = 'N'
                      AND T2.ENABLED_YN = 'Y'
                      )
                 GROUP BY SALE_EMP_NO, DL_MM
       ) A , (
             SELECT TO_CHAR(ADD_MONTHS(TO_DATE('${sEndDt}', 'YYYYMMDD'), -1), 'YYYYMM') AS DL_DT_B1 --검색조건  : 소매일자 YYYYMM
                  , TO_CHAR(ADD_MONTHS(TO_DATE('${sEndDt}', 'YYYYMMDD'), 0), 'YYYYMM')  AS DL_DT    --검색조건  : 소매일자 YYYYMM
               FROM DUAL
           ) B
       GROUP BY A.SALE_EMP_NO
)]]></Query>
</TableData>
</TableDataMap>
