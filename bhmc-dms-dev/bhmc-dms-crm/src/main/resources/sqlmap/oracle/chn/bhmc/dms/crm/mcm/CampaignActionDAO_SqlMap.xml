                                <when test='item.field == "saleKeyNm"'>         CR_0710T.SALE_KEY_NM <choose><when test='item.dir == "asc"'>ASC</when><otherwise>DESC</otherwise></choose>       </when>
                                <when test='item.field == "saleRsltCont"'>      CR_0710T.SALE_RSLT_CONT <choose><when test='item.dir == "asc"'>ASC</when><otherwise>DESC</otherwise></choose>    </when>
