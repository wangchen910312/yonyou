    <!-- 대상자에게 SMS 전송후 SMS 전송일을 수정한다. -->
    <update id="updateCampaignExtrTargetSmsSend" parameterType="CampaignActionVO" >
        /* [chn.bhmc.dms.crm.mcm.service.dao.CampaignActionDAO.updateCampaignExtrTargetSmsSend] */
        UPDATE CR_0710T SET    --고객케어대상자
             UPDT_DT            = SYSDATE                     --수정일자
            ,UPDT_USR_ID        = #{updtUsrId}                --수정자ID
            ,SMS_SEND_DT        = SYSDATE                     --SMS발송일자
            ,SMS_SEND_RSLT_CONT = #{smsSendRsltCont}                --SMS발송결과내용
        WHERE DLR_CD            = #{dlrCd}                    --딜러코드
          AND EXTR_DGRE_CNT     = #{extrDgreCnt}              --추출차수
          AND CUST_NO           = #{custNo}                   --고객번호
    </update>

    <!-- [그룹전송] 대상자에게 SMS 전송후 SMS 전송일을 수정한다. ( SMS 전송 대상자 임시 테이블에서 select insert ) -->
    <update id="updateGroupCampaignExtrTargetSmsSend" parameterType="CampaignActionVO" >
        /* [chn.bhmc.dms.crm.mcm.service.dao.CampaignActionDAO.updateGroupCampaignExtrTargetSmsSend] */
        UPDATE CR_0710T SET    --캠페인대상자
             UPDT_DT            = SYSDATE                     --수정일자
            ,UPDT_USR_ID        = #{updtUsrId}                --수정자ID
            ,SMS_SEND_DT        = SYSDATE                     --SMS발송일자
            ,SMS_SEND_RSLT_CONT = 'Y'                         --SMS발송결과내용
        WHERE DLR_CD            = #{dlrCd}                    --딜러코드
          AND EXTR_DGRE_CNT     = #{extrDgreCnt}              --추출차수
          AND CUST_NO           IN ( SELECT CUST_NO FROM CR_0104T WHERE SEQ = #{smsTmplUserSeq} )
    </update>

    <!-- 대상자 정보를 콜센터로 전송한다. -->
    <select id="selectCallCenterSends" statementType="CALLABLE" parameterType="CampaignActionVO">

         {call SP_CRM_CALLCENTER_SEND (
           #{dlrCd, mode=IN, jdbcType=VARCHAR}
         , #{custExtrTermNo, mode=IN, jdbcType=VARCHAR}
         , #{refKeyNm, mode=IN, jdbcType=VARCHAR}
         , #{extrDgreCnt, mode=IN, jdbcType=VARCHAR}
         , #{bizCd, mode=IN, jdbcType=VARCHAR}
         , #{regUsrId, mode=IN, jdbcType=VARCHAR}
         , #{updtUsrId, mode=IN, jdbcType=VARCHAR}
         , #{result, mode=OUT, jdbcType=VARCHAR}
         )}

    </select>


    <!-- 콜센터 콜처리완료여부(CALL_PROC_END_YN)가 Y 이면 콜결과(CALL_RSLT_CD)를 각 업무 콜결과 (CALL_RSLT_CD) 에 업데이트 해준다. -->
    <update id="updateCallCenRsltCont" parameterType="CampaignActionVO" >
        UPDATE CR_0710T SET                             -- 고객케어대상자
             UPDT_DT            = SYSDATE                     -- 수정일자
            ,UPDT_USR_ID        = #{updtUsrId}                -- 수정자ID
            ,CALL_RSLT_CD       = #{callRsltCd}               -- 콜결과
            ,CALL_CEN_RSLT_DT   = SYSDATE                     -- 콜센터결과일자
        WHERE DLR_CD            = #{dlrCd}                    -- 딜러코드
          AND TARG_CUST_SEQ     = #{targCustSeq}              -- 대상고객일련번호
    </update>


    <!-- 판매결과내용(SALE_RSLT_CONT)을 업데이트 해준다. -->
    <update id="updateSaleRsltCont" parameterType="CampaignActionVO" >
        UPDATE CR_0710T SET                             -- 고객케어대상자
             UPDT_DT            = SYSDATE                     -- 수정일자
            ,UPDT_USR_ID        = #{updtUsrId}                -- 수정자ID
            ,SALE_RSLT_CONT     = #{saleRsltCont}             -- 판매결과내용
        WHERE DLR_CD            = #{dlrCd}                    -- 딜러코드
          AND TARG_CUST_SEQ     = #{targCustSeq}              -- 대상고객일련번호
    </update>


    <!-- 정비결과내용(SER_RSLT_CONT)을 업데이트 해준다. -->
    <update id="updateSerRsltCont" parameterType="CampaignActionVO" >
        UPDATE CR_0710T SET                             -- 고객케어대상자
             UPDT_DT            = SYSDATE                     -- 수정일자
            ,UPDT_USR_ID        = #{updtUsrId}                -- 수정자ID
            ,SER_RSLT_CONT      = #{serRsltCont}              -- 정비결과내용
        WHERE DLR_CD            = #{dlrCd}                    -- 딜러코드
          AND TARG_CUST_SEQ     = #{targCustSeq}              -- 대상고객일련번호
    </update>



	<!-- 조회 조건에 해당하는 캠페인실행자 정보를 조회한다. -->
    <select id="selectActionByKey" parameterType="CampaignActionSearchVO" resultType="CampaignActionVO">
        /* [chn.bhmc.dms.crm.mcm.service.dao.CampaignActionDAO.selectActionByKey] 2017.02.27 */
            SELECT
                    CR_0710T.DLR_CD             --딜러코드
                   ,CR_0710T.MAK_CD             --마케팅코드
                   ,CR_0710T.EXTR_DGRE_CNT      --추출차수
                   ,CR_0710T.CUST_NO            --고객번호
                   ,CR_0710T.CUST_NM            --고객명
                   ,CR_0710T.CUST_EXTR_TERM_NO  --고객추출조건번호
                   ,CR_0710T.TEL_NO             --전화번호
                   ,CR_0710T.HP_NO              --휴대폰번호
                   ,CR_0710T.WECHAT_ID          --위챗ID
                   ,CR_0710T.EMAIL_NM           --이메일명
                   ,CR_0710T.SUNG_NM            --성명
                   ,CR_0710T.CITY_NM            --도시명
                   ,CR_0710T.DIST_NM            --지역명
                   ,CR_0710T.ZIP_CD             --우편번호
                   ,CR_0710T.EXT_ZIP_CD         --확장우편번호
                   ,CR_0710T.ADDR_NM            --주소명
                   ,CR_0710T.ADDR_DETL_CONT     --주소상세내용
                   ,CR_0710T.SMS_SEND_DT        --SMS발송일자
                   ,CR_0710T.SMS_SEND_RSLT_CONT --SMS발송결과내용
                   ,CR_0710T.CALL_CEN_SEND_DT   --콜센터전송일자
                   ,CR_0710T.CALL_RSLT_CD       --콜결과
                   ,CR_0710T.SALE_SEND_DT       --판매전송일자
                   ,CR_0710T.SALE_KEY_NM        --판매키명
                   ,CR_0710T.SALE_RSLT_CONT     --판매결과내용
                   ,CR_0710T.SER_SEND_DT        --정비전송일자
                   ,CR_0710T.SER_KEY_NM         --정비키명
                   ,CR_0710T.SER_RSLT_CONT      --정비결과내용
                   ,CR_0710T.REG_USR_ID         --등록자ID
                   ,CR_0710T.REG_DT             --등록일자
                   ,CR_0710T.UPDT_USR_ID        --수정자ID
                   ,CR_0710T.UPDT_DT            --수정일자
                   ,CR_0710T.CONTACT_NO         --접촉번호
                   ,CR_0710T.CALL_SEQ           --콜일련번호
                   ,CR_0710T.TARG_CUST_SEQ      --대상고객일련번호
                   ,CR_0710T.CALL_CEN_RSLT_DT   --콜센터결과일자
                   ,CR_0701T.START_DT
                   ,CR_0701T.END_DT
             FROM CR_0710T CR_0710T
             INNER JOIN CR_0701T CR_0701T
             ON CR_0710T.MAK_CD = CR_0701T.MAK_CD
             AND CR_0710T.DLR_CD = CR_0701T.DLR_CD
            <where>
                AND CR_0710T.DLR_CD = #{sDlrCd}    --딜러코드
                AND CR_0710T.TARG_CUST_SEQ = #{sTargCustSeq}
            </where>
    </select>


